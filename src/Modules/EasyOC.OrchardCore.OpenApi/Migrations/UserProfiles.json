    {
      "name": "Queries",
      "Queries": [
        {
          "Index": "OrderDetails",
          "Template": "{\r\n  \"query\": {\r\n    \"bool\": {\r\n      \"must\" : {\r\n          \"term\" : { \"Content.ContentItem.ContentType\" : \"TableOrders\" }\r\n      }\r\n    }\r\n  }\r\n}",
          "ReturnContentItems": true,
          "Name": "LucenQueryOrders",
          "Source": "Lucene",
          "Schema": "{\r\n   \"type\":\"ContentItem/TableOrders\"\r\n}"
        },
        {
          "Index": "IDXUserProfiles",
          "Template": "\r\n\r\n{\r\n    \"query\": {\r\n        \"bool\": {\r\n            \"must\": [\r\n                {\r\n                    \"term\": { \"Content.ContentItem.ContentType\": \"Department\" }\r\n                },\r\n                {\r\n                    \"term\": { \"Content.ContentItem.Published\": 'true' }\r\n                },\r\n                {\r\n                    \"term\": { \"Content.ContentItem.Latest\": \"true\" }\r\n                }\r\n            ],\r\n            {% if filters %}\r\n            \"filter\":[\r\n                  {% for filter in filters %} \r\n                \t{\"{{filter.method}}\": { \"{{filter.key}}\": \"{{filter.value}}\" }},\r\n\t\t\t\t  {% endfor %} \r\n            \t]\r\n            {% endif %}\r\n        }\r\n    },\r\n    \"from\":0,\r\n  \t\"size\":10000\r\n}",
          "ReturnContentItems": true,
          "Name": "queryDepartment",
          "Source": "Lucene",
          "Schema": "{\r\n  \"type\":\"contentItem/Department\"\r\n\r\n}"
        },
        {
          "Index": "CRM",
          "Template": "\r\n\r\n{\r\n    \"query\": {\r\n        \"bool\": {\r\n            \"must\": [\r\n                { \"term\": { \"Content.ContentItem.ContentType\": \"Customer\" } },\r\n                { \"term\": { \"Content.ContentItem.Published\": 'true' } },\r\n                {  \"term\": { \"Content.ContentItem.Latest\": \"true\" } }\r\n            ],\r\n            {% if filters %}\r\n            \"filter\":[\r\n                  {% for filter in filters %} \r\n              \t\t{%if filter.key %}\r\n      \t\t\t\t\t{\"{{filter.method}}\": { \"{{filter.key}}\": \"{{filter.value}}\" },\r\n                    {%else%}\r\n                    \t{\"{{filter.method}}\":\"{{filter.value}}\"},\r\n                    {%endif%}\r\n\t\t\t\t  {% endfor %} \r\n            \t]\r\n            {% endif %}\r\n        }\r\n    }\r\n  {% if from %}\r\n    ,\"from\":{{from}}\r\n  {% endif %}\r\n  {%if size %}\r\n   ,\"size\":{{size}}\r\n  {%endif%}  \t\r\n}",
          "ReturnContentItems": true,
          "Name": "crmCustomers",
          "Source": "Lucene",
          "Schema": "{\r\n  \"type\":\"contentItem/Customer\" ,\r\n  \"hasTotal\": true\r\n}"
        }
