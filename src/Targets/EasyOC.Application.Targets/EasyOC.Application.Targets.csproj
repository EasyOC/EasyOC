<Project Sdk="Microsoft.NET.Sdk">
  <!--
    当一个包没有被直接引用时，例如仅通过该捆绑包的引用，
    如果此文件夹被标记为专用，则其生成文件夹中的文件不会在生成时进行评估。
    这可以通过使用“PrivateAssets”属性来定义。
    在这里，只使用项目引用，但打包包时，它们会成为包引用，
    并具有相同的“私有资产”属性。
  -->
  <PropertyGroup>
    <RootNamespace>EasyOC.OrchardCore.Application.Targets</RootNamespace>
  </PropertyGroup>
  <ItemGroup>
    <ProjectReference Include="..\EasyOC.OrchardCore.Targets\EasyOC.OrchardCore.Targets.csproj" PrivateAssets="none" />
    <ProjectReference Include="..\..\Modules\EasyOC.Lucene\EasyOC.Lucene.csproj" PrivateAssets="none" />
    <ProjectReference Include="..\..\Modules\EasyOC.Amis\EasyOC.Amis.csproj" PrivateAssets="none" />

    <ProjectReference Include="..\..\Modules\EasyOC.AntdAdmin\EasyOC.AntdAdmin.csproj" PrivateAssets="none" />
    <ProjectReference Include="..\..\Modules\EasyOC.AuditTrail\EasyOC.AuditTrail.csproj" PrivateAssets="none" />
    <ProjectReference Include="..\..\Modules\EasyOC.FreeSql\EasyOC.FreeSql.csproj" PrivateAssets="none" />
    <ProjectReference Include="..\..\Modules\EasyOC.ContentExtensions\EasyOC.ContentExtensions.csproj" PrivateAssets="none" />
    <ProjectReference Include="..\..\Modules\EasyOC.DynamicTypeIndex\EasyOC.DynamicTypeIndex.csproj" PrivateAssets="none" />
    <ProjectReference Include="..\..\Modules\EasyOC.OpenApi\EasyOC.OpenApi.csproj" PrivateAssets="none" />
    <ProjectReference Include="..\..\Modules\EasyOC.Scripting\EasyOC.Scripting.csproj" PrivateAssets="none" />
    <ProjectReference Include="..\..\Modules\EasyOC.Excel\EasyOC.Excel.csproj" PrivateAssets="none" />
    <ProjectReference Include="..\..\Modules\EasyOC.RDBMS\EasyOC.RDBMS.csproj" PrivateAssets="none" />
    <ProjectReference Include="..\..\Modules\EasyOC.Workflows\EasyOC.Workflows.csproj" PrivateAssets="none" />
    <ProjectReference Include="..\..\Modules\EasyOC.GraphQL\EasyOC.GraphQL.csproj" PrivateAssets="none" />
    <ProjectReference Include="..\..\Modules\OrchardCore.Wechat\OrchardCore.Wechat.csproj" PrivateAssets="none" />
    <ProjectReference Include="..\..\Themes\EasyOCSiteTheme\EasyOCSiteTheme.csproj" PrivateAssets="none" />
    <ProjectReference Include="..\..\Themes\EasyOCVbenAdminTheme\EasyOCVbenAdminTheme.csproj" PrivateAssets="none" />
    <ProjectReference Include="..\..\Themes\EasyOCTheme\EasyOCTheme.csproj" PrivateAssets="none" />
    <!--<ProjectReference Include="..\..\OrchardCore.Modules\OrchardCore.OpenId\OrchardCore.OpenId.csproj" Version="$(OrchardCoreVersion)" PrivateAssets="none"/>-->

    <!--    <ProjectReference Include="..\..\Modules\OrchardCore.Transformalize\OrchardCore.Transformalize.csproj" PrivateAssets="none" />-->

  </ItemGroup>

</Project>



